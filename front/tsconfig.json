{
    "compilerOptions": {
      "target": "ES2022",
      "module": "ESNext",
      "lib": ["DOM", "ES2022"],
      "jsx": "react-jsx",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "moduleResolution": "bundler",
      "forceConsistentCasingInFileNames": true,
      "allowSyntheticDefaultImports": true
    },
    "include": ["src"]
  }